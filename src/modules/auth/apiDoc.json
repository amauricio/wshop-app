{
    "paths": {
      "/auth/token": {
        "post": {
          "tags": ["auth"],
          "summary": "Auth collection.",
          "description": "get response user profile",
          "operationId": "create-token",
          "consumes": ["application/json"],
          "produces": ["application/json"],
          "parameters": [
            {
              "in": "body",
              "name": "body",
              "description": "Request body.",
              "required": true,
              "schema": {
                "$ref": "#/definitions/createToken"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Successful operation"
            },
            "400": {
              "description": "Error: Bad Request"
            },
            "403": {
              "description": "Error: Forbidden"
            },
            "401": {
              "description": "Unauthorized"
            }
          },
          "security": [
            {
              "api_key": []
            }
          ]
        }
      },
      "/auth/user/check": {
        "post": {
          "tags": ["auth"],
          "summary": "Check if user exists.",
          "description": "Check if user was created (exists in database)",
          "operationId": "check-user",
          "consumes": ["application/json"],
          "produces": ["application/json"],
          "parameters": [
            {
              "in": "body",
              "name": "body",
              "description": "Request body.",
              "required": true,
              "schema": {
                "$ref": "#/definitions/checkUser"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Successful operation"
            },
            "400": {
              "description": "Error: Bad Request"
            },
            "403": {
              "description": "Error: Forbidden"
            },
            "401": {
              "description": "Unauthorized"
            }
          },
          "security": [
            {
              "api_key": []
            }
          ]
        }
      }
    },
    "definitions": {
      "createToken": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "required": true,
            "example": "mauricio@wshopnow.com",
            "description": "Username"
          },
          "secret": {
            "type": "string",
            "required": true,
            "example": "1000144",
            "description": "Secret generated by login"
          }
        }
      },
      "checkUser": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "required": true,
            "example": "mauricio@wshopnow.com",
            "description": "Username"
          }
        }
      }
    }
  }
  